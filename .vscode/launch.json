    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test Generate",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--image_dir", "${workspaceFolder}/test_inp/",
                "--save_dir", "${workspaceFolder}/test_out/",
                "--num_superpixels", "25",
                "--algorithm", "watershed",
                "--feature_extractor", "BLIP",
                "--rag"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Test Load",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test_load.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },        
    ]
}